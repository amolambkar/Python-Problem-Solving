   "source": [
    "# take a list\n",
    "a = [1,2,3,4,5,6,7,8,9]\n",
    "\n",
    "# initialize sum as 0\n",
    "sum = 0\n",
    "\n",
    "# using for loop add every element of list to the sum\n",
    "for i in a:\n",
    "    sum += i #sum = sum + i\n",
    "\n",
    "print(sum)"
   ]
